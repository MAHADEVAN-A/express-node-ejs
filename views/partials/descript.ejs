<h1><%=dtitle%></h1>
<% var count = count %>
<% for(var i=1;i<=count;i++){ %>
<div class="container" id="<%= title %><%=i%><%=i%><%=i%>">
    <div class="content">
        <h2><%= details[i-1].content %></h2>
    </div>
    <div class="imge">
        <img src="/assets/<%=image%>/<%=imagesname%>/image<%=i%>.svg" alt="">
    </div>
</div>
<div class="action">
    <button data-modal-target="#modal1<%=i%>">Edit</button>
    <div class="modal" id="modal1<%=i%>">
        <div class="modal-header">
            <div class="title">
                Choose the file:
            </div>
            <button data-close-button class="close-button">
                &times;
            </button>
        </div>
        <div class="modal-body">
            <form id="<%= title %><%=i%>" action="https://mahadevan.herokuapp.com/api/<%= title %>/<%=id%>/<%=i%>" method="POST" enctype="multipart/form-data">
                <label for="content"><h2>Enter the content:</h2></label>
                <input type="text" name="content">
                <input type="file" name="image">
                <button class="<%= title %><%=i%><%=i%>" form="<%= title %><%=i%>" type="submit">SEND</button>
            </form>
        </div>
    </div>
    <div id="overlay">

    </div>
</div>
<% } %>
